{
    "name": "ChatTree",
    "description": "Tree view of your ChatGPT conversations",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "logo16.png",
            "48": "logo48.png",
            "128": "logo128.png"
        }
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "storage",
        "tabs",
        "webRequest",
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "https://chatgpt.com/backend-api/*"
    ]
}
